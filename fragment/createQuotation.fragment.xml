	<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog class="QuotationClass" title="Product Details" contentWidth="80%" contentHeight="80%">
		<content>
			<Bar>
				<contentLeft>
					<Label text="Select Collection"/>
					<ComboBox items="{collectionModel>/}"  selectionChange="handleSelectCollection" valueStateText="Select the Collection"
						placeholder="Enter the Collection">
						<core:Item text="{collectionModel>DCollection_Name}" key="{collectionModel>id}"/>
					</ComboBox>
				</contentLeft>
				<contentMiddle>
					<Label text="Select Category"/>
					<ComboBox items="{categoryModel>/}" selectionChange="handleSelectCategory" valueStateText="Select the Category" placeholder="Enter the Category">
						<core:Item text="{categoryModel>DProduct_Type}" key="{categoryModel>id}"/>
					</ComboBox>
				</contentMiddle>
				<contentRight>
					<Button text="Go" type="Emphasized" press="onPressGo"/>
				</contentRight>
			</Bar>
			<Panel  accessibleRole="Region" backgroundDesign="Transparent" class="sapUiNoContentPadding">
				<headerToolbar>
					<Toolbar>
						<Title text="Products" level="H3" titleStyle="H2" class="sapUiMediumMarginTopBottom"/>
						<!--<ToolbarSpacer></ToolbarSpacer>
						<Button icon="sap-icon://cart" press="handleCartSelectedItems"/>-->
					</Toolbar>
				</headerToolbar>
			<l:BlockLayout background="Dashboard">
