<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog title="Assign User" showHeader="false" class="sapUiSizeCompact">
		<endButton>
			<Button text="Cancel" press="handleCollectionCancelPress"/>
		</endButton>
		<NavContainer width="100%" height="26em">
			<Page showNavButton="false" title="Browse By" showHeader="true">
				<List  noDataText="No data" mode="None">
					<items>
						<StandardListItem title="Collection" type="Navigation" press="handleCollection"/>
						<StandardListItem title="Categories" type="Navigation" press="handleCategory"/>
					</items>
				</List>
			</Page>
			<Page showNavButton="true" title="Collection" navButtonPress="onDeptNavigationBackPress" class="navigationSelectClass">
				<content>
					<List  mode="SingleSelectMaster" selectionChange="handleCollectionSelection">
						<items>
							<StandardListItem title="Hudson"></StandardListItem>
							<StandardListItem title="Hopper"></StandardListItem>
							<StandardListItem title="Chester"></StandardListItem>
							<StandardListItem title="Topanga"></StandardListItem>
							<StandardListItem title="Cuf"></StandardListItem>
							<StandardListItem title="Thompson"></StandardListItem>
						</items>
					</List>
				</content>
			</Page>
			<Page showNavButton="true" title="Categories" navButtonPress="onDeptNavigationBackPress" class="navigationSelectClass">
				<content>
					<List  mode="SingleSelectMaster" selectionChange="handleCollectionSelection">
						<items>
							<StandardListItem title="Lounge Chairs"></StandardListItem>
							<StandardListItem title="Sofas"></StandardListItem>
							<StandardListItem title="Coffe Table"></StandardListItem>
							<StandardListItem title="Bar Tables"></StandardListItem>
							<StandardListItem title="Dining Tables"></StandardListItem>
							<StandardListItem title="Side Tables"></StandardListItem>
						</items>
					</List>
				</content>
			</Page>
			
		</NavContainer>
	</Dialog>
</core:FragmentDefinition>